{
  "name": "@valkyrie-protocol/sdk",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript SDK for Valkyrie Protocol - High-performance distributed messaging",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "ts-node src/examples/basic-example.ts"
  },
  "keywords": [
    "valkyrie",
    "protocol",
    "messaging",
    "rpc",
    "websocket",
    "tcp",
    "distributed",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "RustCI Team <team@rustci.dev>",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rustci/valkyrie-protocol.git",
    "directory": "valkyrie-sdk/javascript"
  },
  "bugs": {
    "url": "https://github.com/rustci/valkyrie-protocol/issues"
  },
  "homepage": "https://docs.valkyrie-protocol.dev",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE*"
  ],
  "dependencies": {
    "ws": "^8.14.0",
    "uuid": "^9.0.0",
    "eventemitter3": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}